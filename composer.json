{
    "name": "itk-dev/os2forms_failed_jobs",
    "description": "Provides list of failed jobs to each form.",
    "type": "drupal-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Yde Granath",
            "email": "my@aarhus.dk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": "^8.0",
        "drupal/advancedqueue": "^1.0",
        "drush/drush": "^10 || ^11",
        "drupal/webform": "^6.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "drupal/coder": "^8.3",
        "mglaman/drupal-check": "^1.4"
    },
    "scripts": {
        "code-analysis/drupal-check": [
            "vendor/bin/drupal-check --deprecations --analysis --exclude-dir=vendor *.* src"
        ],
        "code-analysis": [
            "@code-analysis/drupal-check"
        ],
        "coding-standards-check/phpcs": [
            "vendor/bin/phpcs --standard=phpcs.xml.dist"
        ],
        "coding-standards-check": [
            "@coding-standards-check/phpcs"
        ],
        "coding-standards-apply/phpcs": [
            "vendor/bin/phpcbf --standard=phpcs.xml.dist"
        ],
        "coding-standards-apply": [
            "@coding-standards-apply/phpcs"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
